<%#- html_title "News" %>
<%= stylesheet_link_tag "#{I18n.locale}.css" %>
<%- content_for :main_class, "register" %>
<fieldset>
  <legend>New user</legend>
  <%- form_for @subscriber_user, :url => users_path do |f| %>
    <%= f.text_field :email %>
    <%= f.text_field :email_confirmation %>
    <%= f.submit "Submit" %>
  <%- end %>
</fieldset>
<%= link_to "Back", users_path %>
