Users
<table>
  <thead>
    <th>Username</th>
    <th>Email</th>
    <th>
  </thead>
  <% if @users.size > 0 %>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td>
        </td>
      </tr>
    <% end %>
  <% end %>
</table>